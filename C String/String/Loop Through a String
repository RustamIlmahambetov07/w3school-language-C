#include <stdio.h>

int main(void)
{
    char carName[] = "Volvo";
    int i;

    for (i = 0; i < 5; i++){
        printf("%c\n", carName[i]);
    }
}

//

#include <stdio.h>

int main(void)
{
    char carName[] = "Volvo";
    int length = sizeof(carName) / sizeof(carName[0]);
    int i;

    for (i = 0; i < length; i++){
        printf("%c\n", carName[i]);
    }
}
